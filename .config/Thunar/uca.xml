<?xml encoding="UTF-8" version="1.0"?>
<actions>
<action>
	<icon>utilities-terminal</icon>
	<name>Open Terminal Here</name>
	<unique-id>1557651607347547-1</unique-id>
	<command>exo-open --working-directory %f --launch TerminalEmulator</command>
	<description>Example for a custom action</description>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
	<icon>folder-new</icon>
	<name>Move into New Folder</name>
	<unique-id>1574836283252858-1</unique-id>
	<command>NEW_FOLDER=$(zenity --entry --title &quot;Move to folder&quot; --text &quot;Name of the new folder&quot;); mkdir $NEW_FOLDER; mv %F $NEW_FOLDER</command>
	<description>Selected files will be moved to a new folder</description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>accessories-archiver</icon>
	<name>Extract Here</name>
	<unique-id>1574835206176482-5</unique-id>
	<command>extract %f</command>
	<description>Extract any archive</description>
	<patterns>*.tar.bz2;*.tar.xz;*.tar.gz;*.bz2;*.rar;*.gz;*.tar;*.tbz2;*.tgz;*.zip;*.Z;*.xz;*.7z;*a</patterns>
	<other-files/>
</action>
<action>
	<icon>accessories-archiver</icon>
	<name>Create ZIP Archive</name>
	<unique-id>1576510403417313-3</unique-id>
	<command>zip -r %n.zip %n</command>
	<description>Create a zip archives</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>accessories-archiver</icon>
	<name>Create GZIP Archive</name>
	<unique-id>1576510513626763-2</unique-id>
	<command>tar -czf %n.tar.gz %n</command>
	<description>Create a GZIP archive with tar</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>avidemux</icon>
	<name>Convert to MP4</name>
	<unique-id>1574834909743534-4</unique-id>
	<command>ffmpeg -i %f -codec copy `basename %f .mkv`.mp4</command>
	<description>Convert MKV files to MP4</description>
	<patterns>*.mkv</patterns>
	<video-files/>
</action>
</actions>
